include ArchivosPug/Configuraciones.pug
<!DOCTYPE html>
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="CSS/AboutUs.css")
        title About Us
    body
        include ArchivosPug/NavBar.pug

        main.main
            section.Banner
                p ABOUT US
                img(src="Assets/BannerAbouUs.png", alt="Laptop Social Jobs")

            section.Card2
                section.input 
                div.input__icono--1
                    input(type="text" name="input" placeholder="Google")
                div.input__icono--2
                    input(type="text" name="input" placeholder="Full Name")
                div.input__icono--3
                    input(type="text" name="input" placeholder="User Name")
                div.input__icono--4
                    input(type="text" name="input" placeholder="Email")
                div.input__icono--5
                    input(type="password" name="input" placeholder="Password")
                section.buttonText
                    a(href="Jobs.html") #{botonesText[5]}

        include ArchivosPug/Footer.pug